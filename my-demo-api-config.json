{
  "project": {
    "name": "my-demo-api"
  },
  "environment": {
    "NODE_ENV": "production",
    "API_VERSION": "v1",
    "DEBUG": "false",
    "APP_NAME": "Demo API"
  },
  "wrangler": {
    "compatibility_date": "2024-01-01",
    "compatibility_flags": [
      "nodejs_compat"
    ],
    "vars": {
      "CUSTOM_VAR": "hello-world",
      "FEATURE_FLAG": "enabled"
    },
    "secrets": [
      "DATABASE_URL",
      "JWT_SECRET"
    ]
  },
  "deploy": {
    "auto_deploy": true,
    "environments": [
      "production"
    ]
  }
}