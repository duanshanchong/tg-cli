{
  "project": {
    "name": "{{PROJECT_NAME}}"
  },
  "environment": {
    "NODE_ENV": "production",
    "API_VERSION": "v1",
    "DEBUG": "false"
  },
  "wrangler": {
    "compatibility_date": "2024-01-01",
    "compatibility_flags": [
      "nodejs_compat"
    ],
    "vars": {
      "CUSTOM_VAR": "custom-value"
    },
    "secrets": [
      "DATABASE_URL",
      "JWT_SECRET"
    ]
  },
  "deploy": {
    "auto_deploy": true,
    "environments": [
      "production"
    ]
  }
} 